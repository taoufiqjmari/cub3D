int     ret_of_split(char **str)
{
    int		i;
    i = 0;
    while (str[i])
        i++;
    return (i);
}

void   free_dpointer(char **str, int i)
{
    if (str)
    {
        while (i >= 0)
        {
            free(str[i]);
            str[i] = NULL;
            i--;
        }
        free(str);
        str = NULL;
    }
}